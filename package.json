    "@storybook/react": "^6.5.14",
    "@types/jest": "^27.4.1",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.7",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vanilla-extract/css": "^1.9.2",
