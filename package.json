    "jest": "^27.5.1",
    "npm-check-updates": "^16.5.6",
    "prettier": "^2.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
